{
  "name": "fslockjs",
  "version": "2.1.0",
  "description": "Easy to use file system queue with locking and events. Provide Asynchronous utilities for Directories and File",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "mocha src/**/*.spec.js --recursive",
    "prepublish": "tsc --build tsconfig.json",
    "clean": "git clean -dfqX -- ./node_modules **/{dist,node_modules}/",
    "compile": "tsc --build tsconfig.json",
    "compile:clean": "tsc --build tsconfig.json --clean",
    "watch": "tsc --build tsconfig.json --watch",
    "format": "prettier --write \"**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alex-Werner/FSlock.git"
  },
  "author": "Alex Werner <obusco@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alex-Werner/FSlock/issues"
  },
  "keywords": [
    "queue",
    "job",
    "async",
    "task",
    "concurant",
    "storage",
    "fs",
    "file",
    "directory",
    "nodejs"
  ],
  "homepage": "https://github.com/Alex-Werner/FSlock#readme",
  "devDependencies": {
    "@types/download": "^6.2.4",
    "@types/fs-extra": "^9.0.6",
    "@types/node": "^14.14.19",
    "@types/slocket": "^1.0.1",
    "chai": "^4.2.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "mocha": "^8.2.1",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "download": "^8.0.0",
    "fs-extra": "^9.0.1",
    "lodash.map": "^4.6.0",
    "slocket": "^1.0.5"
  }
}
